<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <io.github.loop_x.yummywakeup.view.YummyTextView
        android:layout_width="51.5dp"
        android:layout_height="24.5dp"
        android:layout_marginRight="213.5dp"
        android:layout_marginTop="112dp"
        android:background="@color/loopX_2"
        android:gravity="center"
        android:text="@string/volume"
        android:textColor="@color/loopX_3" />

    <SeekBar
        android:id="@+id/sb_alarm_volume"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="10dp" />

    <io.github.loop_x.yummywakeup.view.YummyTextView
        android:layout_width="51.5dp"
        android:layout_height="24.5dp"
        android:layout_marginRight="213.5dp"
        android:background="@color/loopX_2"
        android:gravity="center"
        android:text="@string/vibration"
        android:textColor="@color/loopX_3" />

    <ToggleButton
        android:id="@+id/sb_alarm_vibration"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="10dp" />

    <io.github.loop_x.yummywakeup.view.YummyTextView
        android:layout_width="51.5dp"
        android:layout_height="24.5dp"
        android:background="@color/loopX_2"
        android:gravity="center"
        android:text="@string/Ringtone"
        android:textColor="@color/loopX_3" />

    <ListView
        android:id="@+id/lv_ringtone_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:choiceMode="singleChoice"/>

</merge>